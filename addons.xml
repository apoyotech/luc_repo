<addon id="repository.luc_repo" name="luc_repo" version="1.0.0" provider-name="luc_kodi">
   <extension point="xbmc.addon.repository" name="Mijn Repository">
		<dir>
			<info compressed="false">https://raw.githubusercontent.com/apoyotech/luc_repo/refs/heads/main/addons.xml</info>
			<checksum>https://raw.githubusercontent.com/apoyotech/luc_repo/refs/heads/main/addons.xml.md5</checksum>
			<datadir zip="true">https://raw.githubusercontent.com/apoyotech/luc_repo/refs/heads/main/</datadir>
		</dir>
	</extension>
	<extension point="xbmc.addon.metadata">
    <summary lang="en">luc_repo</summary>
    <description lang="en">luc_repo for open source Kodi</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.program.luc_kodi.wizard" name="luc_kodi.wizard" version="1.0.0" provider-name="[COLOR red]apoyotech[/COLOR]">
	<requires>
		<import addon="xbmc.python" version="3.0.0"/>
		<import addon="script.module.requests" version="2.31.0" />
	        <import addon="script.module.certifi" version="2023.5.7" />
	        <import addon="script.module.chardet" version="5.1.0" />
	        <import addon="script.module.idna" version="3.4.0" />
	        <import addon="script.module.urllib3" version="2.1.0" />
	        <import addon="script.module.six" version="1.16.0" />
	        <import addon="script.module.simplejson" version="3.19.1" />
	        <import addon="script.module.soupsieve" version="2.4.1" />
	        <import addon="script.module.inputstreamhelper" version="0.6.1" />
	        <import addon="script.module.beautifulsoup4" version="4.9.3" />
	        <import addon="script.module.kodi-six" version="0.1.3.1" />
	</requires>
	<extension point="xbmc.python.pluginsource" library="addon.py">
		<provides>executable</provides>
	</extension>
	<extension point="xbmc.service" library="service.py" />
	<extension point="xbmc.addon.metadata">
		<summary lang="en_GB">luc_kodi.wizard by apoyotech</summary>
		<description lang="en_GB">This simple plugin.program.wizard it serves to create and install backups and maintenance in open source kodi</description>
		<disclaimer lang="en_GB">This Addon contains no content</disclaimer>
		<language>en</language>
		<platform>all</platform>
		<license>GPL-2.0-or-later</license>
		<forum></forum>
		<website></website> 
		<email></email>
		<source></source>
		<news>Initial release 1.0.0 -Public.
		</news>
		<assets>
				<icon>resources/icon.png</icon>
				<fanart>resources/fanart.jpg</fanart>
				<banner></banner>
				<clearlogo></clearlogo>
				<screenshot></screenshot>
		</assets>
	</extension>
</addon>

